#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/nuxt@4.0.3_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler-_82a75bef28b9bd68caa66708dfd7acb5/node_modules/nuxt/bin/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/nuxt@4.0.3_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler-_82a75bef28b9bd68caa66708dfd7acb5/node_modules/nuxt/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/nuxt@4.0.3_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler-_82a75bef28b9bd68caa66708dfd7acb5/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/nuxt@4.0.3_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler-_82a75bef28b9bd68caa66708dfd7acb5/node_modules/nuxt/bin/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/nuxt@4.0.3_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler-_82a75bef28b9bd68caa66708dfd7acb5/node_modules/nuxt/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/nuxt@4.0.3_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler-_82a75bef28b9bd68caa66708dfd7acb5/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../../bin/nuxt.mjs" "$@"
fi
