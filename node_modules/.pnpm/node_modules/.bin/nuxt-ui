#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+ui@3.3.2_@babel+parser@7.28.3_@netlify+blobs@9.1.2_change-case@5.4.4_db0@0.3.2_be_8ec78a0fd1378e9592b1b0325527dcf5/node_modules/@nuxt/ui/cli/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+ui@3.3.2_@babel+parser@7.28.3_@netlify+blobs@9.1.2_change-case@5.4.4_db0@0.3.2_be_8ec78a0fd1378e9592b1b0325527dcf5/node_modules/@nuxt/ui/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+ui@3.3.2_@babel+parser@7.28.3_@netlify+blobs@9.1.2_change-case@5.4.4_db0@0.3.2_be_8ec78a0fd1378e9592b1b0325527dcf5/node_modules/@nuxt/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+ui@3.3.2_@babel+parser@7.28.3_@netlify+blobs@9.1.2_change-case@5.4.4_db0@0.3.2_be_8ec78a0fd1378e9592b1b0325527dcf5/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+ui@3.3.2_@babel+parser@7.28.3_@netlify+blobs@9.1.2_change-case@5.4.4_db0@0.3.2_be_8ec78a0fd1378e9592b1b0325527dcf5/node_modules/@nuxt/ui/cli/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+ui@3.3.2_@babel+parser@7.28.3_@netlify+blobs@9.1.2_change-case@5.4.4_db0@0.3.2_be_8ec78a0fd1378e9592b1b0325527dcf5/node_modules/@nuxt/ui/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+ui@3.3.2_@babel+parser@7.28.3_@netlify+blobs@9.1.2_change-case@5.4.4_db0@0.3.2_be_8ec78a0fd1378e9592b1b0325527dcf5/node_modules/@nuxt/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+ui@3.3.2_@babel+parser@7.28.3_@netlify+blobs@9.1.2_change-case@5.4.4_db0@0.3.2_be_8ec78a0fd1378e9592b1b0325527dcf5/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/ui/cli/index.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/ui/cli/index.mjs" "$@"
fi
