#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.1.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@1.30.1_terse_4ff7d38eb198bd532b375b0f5d80929b/node_modules/@nuxt/devtools/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.1.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@1.30.1_terse_4ff7d38eb198bd532b375b0f5d80929b/node_modules/@nuxt/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.1.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@1.30.1_terse_4ff7d38eb198bd532b375b0f5d80929b/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.1.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@1.30.1_terse_4ff7d38eb198bd532b375b0f5d80929b/node_modules/@nuxt/devtools/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.1.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@1.30.1_terse_4ff7d38eb198bd532b375b0f5d80929b/node_modules/@nuxt/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/@nuxt+devtools@2.6.2_vite@7.1.3_@types+node@24.3.0_jiti@2.5.1_lightningcss@1.30.1_terse_4ff7d38eb198bd532b375b0f5d80929b/node_modules:/home/victor_v/projects/nuxt-laravel-docker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.mjs" "$@"
else
  exec node  "$basedir/../../cli.mjs" "$@"
fi
